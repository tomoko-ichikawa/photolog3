<h1>ユーザー詳細</h1>
<% @users.each do |user| %>
  <p><%= image_tag user.avatar.to_s %></p>
<% end %>
  <p>name : <%= @user.name %></p>
  <p>profile: <%= @user.profile %>

<hr>

<% @photos.each do |photo| %>
  <hr>
  <p><%= image_tag photo.image.to_s %></p>
  <p><span>emai: </span><%= @user.email %></p>
  <p><span>投稿内容: </span><%=link_to photo.caption, photo_path(photo.id) %></p>
<% end %>
